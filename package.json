        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "prettier": "^3.1.1"
    },
    "jest": {
        "collectCoverageFrom": [
          "src/**/*.{js,jsx,ts,tsx}",
          "!src/setupTests.ts",
          "!src/index.tsx"
